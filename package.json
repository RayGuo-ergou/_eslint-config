{
  "name": "@ergou_rrrr/eslint-config",
  "type": "module",
  "version": "0.0.7",
  "packageManager": "pnpm@8.8.0",
  "description": "Ray's ESLint config",
  "author": {
    "name": "Ray",
    "email": "rayguo.rf@gmail.com",
    "url": "http://ruifengguo.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/RayGuo-ergou/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm",
    "dev": "tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "pnpm run stub && eslint .",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "2.3.4",
    "@unocss/eslint-plugin": "^0.58.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@ergou_rrrr/eslint-config": "workspace:*",
    "@stylistic/eslint-plugin-migrate": "^1.5.0",
    "@types/eslint": "^8.44.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.3",
    "bumpp": "^9.2.0",
    "eslint": "^8.55.0",
    "eslint-flat-config-viewer": "^0.1.3",
    "eslint-plugin-sort-keys": "^2.3.5",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "sucrase": "^3.34.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.0.1"
  }
}
